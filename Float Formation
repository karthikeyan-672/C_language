#include <stdio.h>
#include <math.h>  

int main() {
    float value;


   scanf("%f", &value);
    printf("%f\n", value);         
    printf("%.4f\n", value);       
    printf("%.2f\n", value);      
    printf("%.0f\n", round(value));

    return 0;
}
