#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    float speed, distance, time, time_to_reach;

    // Reading inputs
    scanf("%f", &speed);
    scanf("%f", &distance);
    scanf("%f", &time);

    // Time to reach the light in seconds
    time_to_reach = (distance / speed) * 3600;  // convert hours to seconds

    if (time_to_reach < time)
        printf("Yes\n");
    else
        printf("No\n");

    return 0;
}
