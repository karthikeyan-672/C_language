#include <stdio.h>
#include <stdio.h>
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n,p;
    scanf("%d",&n);
    for(int i=1;i<=n;++i){
        p=n*i;
        printf("%d * %d = %d\n",n,i,p);
    }
    return 0;
}
