#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>
#define PI 3.1416
int main() {
    float l, b, h, r,v1,v2,v3,v4;
    scanf("%f", &l);
    scanf("%f", &b);
    scanf("%f", &h);
    scanf("%f", &r);

     v1 = l * b * h;
    v2 = PI * r * r * h;
    v3 = (PI * r * r * h) / 3.0;
    v4 = (4.0 / 3.0) * PI * r * r * r;

    printf("Volume of Cubiod=%.2f\n", floor(v1 * 100) / 100);
    printf("Volume of Cylinder=%.2f\n", floor(v2 * 100) / 100);
    printf("Volume of Cone=%.2f\n", floor(v3 * 100) / 100);
    printf("Volume of Shpere=%.2f\n", floor(v4 * 100) / 100);

    return 0;
}
