#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>
#include <stdio.h>
#include <math.h>

int main() {
    int t, v;
    float wcf;
    scanf("%d", &t);
    scanf("%d", &v);
    wcf = 35.74 + 0.6215 * t + (0.4275 * t - 35.75) * pow(v, 0.16);
    printf("%.2f\n", wcf);

    return 0;
}
